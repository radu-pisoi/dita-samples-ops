                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" text-align="center" display-align="before"><fo:block>Thrust in vacuum</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" text-align="center" display-align="before"><fo:block>19482 kN</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block/></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" text-align="center" display-align="before"><fo:block>Height</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" text-align="center" display-align="before"><fo:block>53.7 m</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>Measured from lowest to highest point</fo:block></fo:table-cell>

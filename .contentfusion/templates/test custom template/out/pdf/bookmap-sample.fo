                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" text-align="center" display-align="before"><fo:block>9.78 m</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>Measured from widest point</fo:block></fo:table-cell>
